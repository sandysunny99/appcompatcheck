# NPM Configuration for AppCompatCheck
# This configuration is required for React 19 compatibility with Radix UI

# Allow legacy peer dependencies (required for React 19 + Radix UI v1.x)
legacy-peer-deps=true

# Disable strict peer dependency checking
strict-peer-dependencies=false

# Disable engine strict checking
engine-strict=false

# Auto install peer dependencies
auto-install-peers=true
